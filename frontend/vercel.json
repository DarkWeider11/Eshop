{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "https://backend.vercel.app/:path"
      },
      {
        "src": "/(.*)",
        "dest": "/build/index.html"
      }
    ]
  }
  