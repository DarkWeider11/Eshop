{
  "builds": [{
      "src": "src/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "output": "handler.py"
      }
  }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/wsgi.py"
      }
  ]
}
